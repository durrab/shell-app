    "subapp-server": "^1.3.15",
    "subapp-web": "^1.0.50",
    "webpack-hook-plugin": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@xarc/app-dev": "^10.0.11",
    "@xarc/run": "^1.0.5",
    "axios-mock-adapter": "^1.19.0",
    "babel-jest": "^26.1.0",
